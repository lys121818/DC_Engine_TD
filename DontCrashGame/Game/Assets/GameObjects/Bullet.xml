<?xml version="1.0" encoding="utf-8"?>

<GameObject name ="Bullet">
	<TransformComponent>
		<Position x = "0" y = "0" />
		<Dimensions width = "10" height = "5" />
	</TransformComponent>

	<ImageRenderComponent>
		<State name ="Fire">
			<ImageSource fileName = "Buttons.png"/>
			<Frame x = "0" y = "40" />
			<Dimensions width = "100" height = "40" />
		</State>
	</ImageRenderComponent>
	
	<SoundComponent>
		<State name = "Spawn">
			<SoundSource fileName = "Audios/Sound/Fire.ogg"/>
		</State>
	</SoundComponent>
	
	<TriggerBoxComponent>
		<Size x = "0.0f" y = "0.0f"/>
	</TriggerBoxComponent>
</GameObject>